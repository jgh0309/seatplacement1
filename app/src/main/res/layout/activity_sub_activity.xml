<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.iot.lee_pc.seatplacement.sub_activity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="319dp"
        android:layout_height="284dp"
        android:layout_marginLeft="-15dp"
        android:layout_marginTop="-20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/front2"/>

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintLeft_toRightOf="@+id/imageButton5"
        android:layout_marginLeft="12dp"
        />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        app:layout_constraintLeft_toRightOf="@+id/imageButton4"
        android:layout_marginLeft="11dp"/>

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        android:layout_marginLeft="37dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        android:layout_marginLeft="12dp"/>

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        app:layout_constraintLeft_toRightOf="@+id/imageButton"
        android:layout_marginLeft="11dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"/>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="75dp"
        android:layout_height="65dp"
        app:srcCompat="@android:color/holo_blue_bright"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="37dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

</android.support.constraint.ConstraintLayout>
